{
  "name": "easyfin-gap",
  "version": "1.0.0",
  "description": "Basic packages and scripts to start app developing in react, redux and webpack",
  "main": "webpack.config.js",
  "scripts": {
    "start": "./start.sh",
    "server:dev": "webpack serve --mode development",
    "server:prod": "NODE_ENV=production npm run build && http-server ./build",
    "build": "webpack --progress --mode production",
    "test": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js",
    "test:coverage": "BABEL_DISABLE_CACHE=1 jest --config ./jest/jest.config.js --coverage",
    "eslint": "eslint --quiet ./source/**/*.js ./source/**/**/*.js ./source/**/*.jsx ./source/**/**/*.jsx",
    "stats": "BABEL_ENV=production webpack --profile --json > bundle-stats.json && webpack-bundle-analyzer bundle-stats.json -p 8888 -r bundle-report.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artemdemo/react-webpack-starter.git"
  },
  "author": "Artem Demo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artemdemo/react-webpack-starter/issues"
  },
  "homepage": "https://github.com/artemdemo/react-webpack-starter#readme",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@types/gapi.auth2": "0.0.52",
    "@types/jest": "^25.2.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "^7.0.28",
    "@types/styled-components": "^5.1.7",
    "@types/yup": "^0.29.3",
    "axios": "^0.19.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.2",
    "date-fns": "^2.13.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^6.0.0",
    "formik": "^2.1.4",
    "history": "^4.10.1",
    "html-webpack-plugin": "^4.2.0",
    "jest-cli": "^25.3.0",
    "lodash": "^4.17.15",
    "match-sorter": "^4.2.0",
    "nanoevents": "^5.1.8",
    "nanoid": "^3.1.10",
